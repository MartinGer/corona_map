{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
		"jest": true
    },
    "extends": "airbnb",
	"plugins": ["jest"],
    "rules": {
		"react/jsx-filename-extension": [2, { 'extensions': ['.js', '.jsx', '.ts', '.tsx'] }],
		"linebreak-style": 0,
		"import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    }
}